<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Price_Override_Relevant__c</fullName>
    <description>If threre is any discount on the Order flag the Order as Price Override Relevant = True</description>
    <externalId>false</externalId>
    <formula>IF(OR(Bonus_Cartons__c &gt; 1, Bonus_Unit__c &gt; 1, Total_Discount__c &gt;1), &#39;True&#39;, &#39;False&#39;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Price Override Relevant</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
